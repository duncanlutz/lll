<div class="attorneys-page-wrapper">
    <div class="attorneys-list-wrapper">
        {.repeated section items}
        <a href={fullUrl} id="post-{id}" class="attorneys-list-item {@|item-classes}" data-item-id="{id}">
            <img src="{customContent.listImage.assetUrl}" class="attorneys-list-image" alt="Image of attorney {title}" />
            <h3 class="attorney-list-name">{title}</h3>
        </a>
        {.end}
        <div class="attorneys-list-bg-color"></div>
    </div>
</div>